<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 3.0//EN"
    "http://tuckey.org/res/dtds/urlrewrite3.0.dtd">
<urlrewrite>
  	<rule>
    	<from>^(.*)/statement/(.*)$</from>
    	<to last="true">$1/statementbyuri?uri=$2</to>
    </rule>
    <rule>
    	<from>^(.*)/conceptdomainbinding/(.*)$</from>
    	<to last="true">$1/conceptdomainbyuri?uri=$2</to>
    </rule>
    <rule>
    	<from>^(.*)/valueset/.*/definition/(.*)$</from>
    	<to last="true">$1/valuesetdefinitionbyuri?uri=$2</to>
    </rule>
</urlrewrite>
